{
    "name": "vairogs/bundle",
    "type": "symfony-bundle",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Vairogs\\Bundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "require": {
        "php": ">=8.3",
        "doctrine/collections": "*",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "symfony/http-kernel": "*",
        "vairogs/functions": "*"
    },
    "suggest": {
        "vairogs/cache": "*",
        "vairogs/doctrine-tools": "*",
        "vairogs/mapper": "*",
        "vairogs/settings": "*",
        "vairogs/audit": "*"
    }
}
