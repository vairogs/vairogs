{
    "name": "vairogs/bundle",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.4",
        "api-platform/metadata": "*",
        "api-platform/symfony": "*",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "symfony/event-dispatcher": "*",
        "symfony/finder": "*",
        "symfony/http-kernel": "*",
        "symfony/serializer": "*",
        "vairogs/functions-iteration": "self.version",
        "vairogs/functions-local": "self.version",
        "vairogs/functions-php": "self.version",
        "vairogs/functions-text": "self.version"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "suggest": {
        "doctrine/dbal": "*",
        "doctrine/migrations": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Vairogs\\Bundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}