{
    "name": "vairogs/functions-local",
    "description": "A PHP library providing utility functions for working with the local filesystem, environment variables, system resources, and more",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "filesystem",
        "environment",
        "system",
        "resources",
        "file",
        "directory",
        "vairogs",
        "symfony"
    ],
    "authors": [
        {
            "name": "Davis Zalitis (k0d3r1s)",
            "homepage": "https://me.k0d3r1s.com"
        }
    ],
    "homepage": "https://github.com/vairogs/vairogs",
    "require": {
        "php": ">=8.4",
        "composer-runtime-api": "^2",
        "symfony/process": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Vairogs\\Functions\\Local\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
