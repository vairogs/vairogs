{
    "name": "vairogs/functions-date",
    "description": "A PHP library providing enhanced date and time manipulation functions with improved validation and formatting capabilities",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "date",
        "time",
        "formatting",
        "validation",
        "vairogs",
        "symfony"
    ],
    "authors": [
        {
            "name": "Davis Zalitis (k0d3r1s)",
            "homepage": "https://me.k0d3r1s.com"
        }
    ],
    "homepage": "https://github.com/vairogs/vairogs",
    "require": {
        "php": ">=8.4",
        "vairogs/functions-php": "self.version",
        "vairogs/functions-text": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Vairogs\\Functions\\Date\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
