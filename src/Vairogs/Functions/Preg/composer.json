{
    "name": "vairogs/functions-preg",
    "description": "A PHP library providing enhanced regular expression (preg_*) functions with improved error handling and UTF-8 support",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "regex",
        "regular expression",
        "preg",
        "pattern matching",
        "utf-8",
        "vairogs",
        "symfony"
    ],
    "authors": [
        {
            "name": "Davis Zalitis (k0d3r1s)",
            "homepage": "https://me.k0d3r1s.com"
        }
    ],
    "homepage": "https://github.com/vairogs/vairogs",
    "require": {
        "php": ">=8.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Vairogs\\Functions\\Preg\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
