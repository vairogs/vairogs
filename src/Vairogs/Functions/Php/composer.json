{
    "name": "vairogs/functions-php",
    "description": "A PHP library providing enhanced PHP utility functions for reflection, introspection, and general PHP programming tasks",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "php",
        "utility",
        "reflection",
        "introspection",
        "system",
        "class",
        "method",
        "vairogs",
        "symfony"
    ],
    "authors": [
        {
            "name": "Davis Zalitis (k0d3r1s)",
            "homepage": "https://me.k0d3r1s.com"
        }
    ],
    "homepage": "https://github.com/vairogs/vairogs",
    "require": {
        "php": ">=8.4",
        "ext-random": "*",
        "vairogs/functions-text": "self.version"
    },
    "suggest": {
        "doctrine/persistence": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Vairogs\\Functions\\Php\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}