{
    "name": "vairogs/functions",
    "type": "library",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Vairogs\\Component\\Functions\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "require": {
        "php": ">=8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-random": "*",
        "composer-runtime-api": "^2",
        "symfony/http-foundation": "*",
        "symfony/intl": "*",
        "symfony/routing": "*",
        "symfony/type-info": "*"
    },
    "suggest": {
        "ext-gmp": "*"
    }
}
