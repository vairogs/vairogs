{
    "name": "vairogs/doctrine-tools",
    "type": "library",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Vairogs\\Component\\DoctrineTools\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "require": {
        "php": ">=8.3",
        "doctrine/dbal": "*",
        "doctrine/orm": "*",
        "symfony/config": "*",
        "symfony/dependency-injection": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/serializer": "*",
        "symfony/uid": "*",
        "vairogs/bundle": "*"
    },
    "conflict": {
        "doctrine/dbal": "<4.0"
    }
}
