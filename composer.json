{
    "name": "vairogs/vairogs",
    "type": "library",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Vairogs\\": "src/Vairogs/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "replace": {
        "vairogs/bundle": "self.version",
        "vairogs/cache": "self.version",
        "vairogs/functions": "self.version",
        "vairogs/mapper": "self.version",
        "vairogs/settings": "self.version"
    },
    "require": {
        "php": ">=8.3",
        "doctrine/dbal": "*"
    },
    "require-dev": {
    },
    "conflict": {
        "doctrine/dbal": "<4.0"
    }
}
