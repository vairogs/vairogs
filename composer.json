{
    "name": "vairogs/vairogs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vairogs\\": "src/Vairogs/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "replace": {
        "vairogs/functions": "self.version",
        "vairogs/settings": "self.version",
        "vairogs/bundle": "self.version"
    }
}
