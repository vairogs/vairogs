{
    "name": "vairogs/vairogs",
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "php": ">=8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-random": "*",
        "ext-xmlreader": "*",
        "ext-zip": "*",
        "api-platform/core": "*",
        "api-platform/doctrine-common": "*",
        "api-platform/doctrine-orm": "*",
        "api-platform/metadata": "*",
        "api-platform/openapi": "*",
        "api-platform/state": "*",
        "api-platform/symfony": "*",
        "api-platform/validator": "*",
        "composer-runtime-api": "^2",
        "doctrine/collections": "*",
        "doctrine/dbal": "*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/migrations": "*",
        "doctrine/orm": "*",
        "doctrine/persistence": "*",
        "phpdocumentor/reflection-docblock": "*",
        "psr/log": "*",
        "symfony/config": "*",
        "symfony/console": "*",
        "symfony/dependency-injection": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/event-dispatcher": "*",
        "symfony/finder": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/http-client-contracts": "*",
        "symfony/http-foundation": "*",
        "symfony/http-kernel": "*",
        "symfony/intl": "*",
        "symfony/process": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/routing": "*",
        "symfony/security-bundle": "*",
        "symfony/security-core": "*",
        "symfony/security-http": "*",
        "symfony/serializer": "*",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/translation-contracts": "*",
        "symfony/type-info": "*",
        "symfony/uid": "*",
        "symfony/validator": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpunit/phpunit": "*",
        "roave/security-advisories": "dev-latest"
    },
    "replace": {
        "vairogs/bundle": "self.version",
        "vairogs/cache-warmer": "self.version",
        "vairogs/doctrine-tools": "self.version",
        "vairogs/functions": "self.version",
        "vairogs/mapper": "self.version",
        "vairogs/php-cs-fixer-custom-fixers": "self.version",
        "vairogs/spx-profiler": "self.version",
        "spaghetti/xlsx-parser": "self.version"
    },
    "conflict": {
        "api-platform/symfony": "<4.0.0",
        "doctrine/dbal": "<4.0",
        "friendsofphp/php-cs-fixer": "<3.57.0",
        "symfony/framework-bundle": "<7.2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "autoload": {
        "psr-4": {
            "Vairogs\\": "src/Vairogs/",
            "Spaghetti\\": "src/Spaghetti/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vairogs\\Tests\\": "tests/Vairogs/Tests/",
            "Spaghetti\\Tests\\": "tests/Spaghetti/Tests/",
            "Vairogs\\Assets\\": "tests/Vairogs/Assets/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
